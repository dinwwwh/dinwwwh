{
  "name": "@unnoq/monorepo",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.10.0",
  "author": {
    "name": "unnoq",
    "email": "contact@unnoq.com",
    "url": "https://unnoq.com"
  },
  "scripts": {
    "build": "pnpm run -r build",
    "preview": "pnpm run --parallel preview",
    "check": "pnpm run -r check && tsc -b",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --coverage --ui",
    "lint": "eslint --max-warnings=0 .",
    "lint:fix": "eslint --max-warnings=0 . --fix",
    "sherif": "pnpm dlx sherif",
    "sherif:fix": "pnpm run sherif --fix",
    "knip": "pnpm dlx knip --production",
    "knip:fix": "pnpm run knip --production --fix --allow-remove-files"
  },
  "devDependencies": {
    "@antfu/eslint-config": "latest",
    "@eslint-react/eslint-plugin": "latest",
    "@types/node": "latest",
    "@unnoq/unplugin": "latest",
    "@vitest/coverage-v8": "latest",
    "@vitest/ui": "latest",
    "eslint-plugin-format": "latest",
    "eslint-plugin-react-hooks": "latest",
    "eslint-plugin-react-refresh": "latest",
    "typescript": "latest",
    "vite": "latest",
    "vitest": "latest"
  }
}
