{
  "name": "@dinwwwh/typescript-vite-node-app",
  "type": "module",
  "version": "0.0.0",
  "author": {
    "name": "dinwwwh",
    "email": "dinwwwh@gmail.com",
    "url": "https://dinwwwh.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/dinwwwh/dinwwwh",
  "repository": {
    "type": "git",
    "url": "https://github.com/dinwwwh/dinwwwh.git",
    "directory": "examples/typescript-vite-node-app"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "dinwwwh"
  ],
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./*": "./dist/*"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "dev": "VITE_ON_SUCCESS='node ./dist/main.js' vite build --watch",
    "build": "vite build",
    "preview": "node ./dist/main.js",
    "type:check": "tsc -b",

    "example": "vite-node scripts/example.ts"
  },
  "devDependencies": {
    "@dinwwwh/tsconfig": "latest",
    "@types/node": "latest",
    "typescript": "latest",
    "vite": "latest",
    "vite-node": "latest",
    "vite-plugin-on-success": "latest",
    "vite-tsconfig-paths": "latest",
    "vitest": "latest"
  }
}
